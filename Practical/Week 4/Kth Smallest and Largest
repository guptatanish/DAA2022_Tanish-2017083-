#include <iostream>
#include <bits/stdc++.h>
using namespace std;


int main()
{
    int n;
    cin >> n;
    int a[n];
    for(int i=0;i<n;i++)
        cin >> a[i];

    int k;
    cin >> k;

    sort(a,a+n);

    cout << k << "th smallest : " << a[k-1] << endl;
    cout << k << "th largest : " << a[n-k] << endl;

    return 0;
}
